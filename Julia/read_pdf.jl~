#array_to_dict.jl
function to_tuple(X)
    if typeof(X) <: Array
        tmp = [ to_tuple(x) for x in X ]
        x = (tmp[1:end]...)
    else
        return X
    end
end


function array_to_dict(a)
    local d = Dict()
    for p in a
        t = to_tuple( p[1] )
        d[t] = p[2]
    end
    return d
end
